        "a = {'0':0.4, '1':0.8}\n",
        "x = list(a.keys())\n",
        "y = list(a.values())\n",
        "plt.grid()\n",
        "plt.stem(x,y,use_line_collection = True)\n",
        "plt.xlabel(\"$x$\")\n",
        "plt.ylabel(\"$p_{X}(x)$\")\n",
        "plt.title(\"PMF of $X$\")\n",
        "plt.show()\n"
